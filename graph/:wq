class Graph(object):
    def __init__(self):
        self.adjacency_list = {}

    def add_node(self, node):
        self.adjacency_list[node] = set()

    def add_edge(self, start, end):
        self.adjacency_list[start].add(end)

    def get_successors(self, start):
        return self.adjacency_list[start]


